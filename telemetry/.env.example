# Tinybird credentials — copy to .env.local and fill in values
# NEVER commit .env.local — it is gitignored

# Append-only token for hook ingestion (datasource writes)
TB_INGEST_TOKEN=your_ingest_token_here

# Read-only token scoped to specific pipes (SessionStart query)
TB_READ_TOKEN=your_read_token_here

# Tinybird host (used by SDK and tinybird CLI)
TB_HOST=https://api.tinybird.co

# Combined token for tinybird CLI (build/deploy) — NOT used by hooks
TB_TOKEN=your_admin_token_here
